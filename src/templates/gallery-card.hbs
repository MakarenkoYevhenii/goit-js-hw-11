{{#each this}}
<div class="photo-card">
  <img src={{webformatURL}} alt={{tags}} loading="lazy" class="kartinka"/>
  <div class="info">
    <p class="info-item">
      <b>likes:{{likes}}</b>
    </p>
    <p class="info-item">
      <b>views:{{views}}</b>
    </p>
    <p class="info-item">
      <b>comments:{{comments}}</b>
    </p>
    <p class="info-item">
      <b>Downloads:{{downloads}}</b>
    </p>
  </div>
</div>
{{/each}}